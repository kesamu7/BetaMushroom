    @model IEnumerable<BetaMushroom.Models.ProfileSearchResultViewModel>

@{
    ViewData["Title"] = "Result";
}

<h2>Search Results</h2>


@foreach (var item in Model)
{


    <div class="panel panel-default">
        <div class="panel-heading">
            <h3 class="panel-title">@Html.DisplayFor(modelItem => item.UserName) - @Html.DisplayFor(modelItem => item.Age) - @Html.DisplayFor(modelItem => item.Gender)</h3>
        </div>

        <div class="panel-body">
            <div class="row">
                <div class="col-md-8">
                    @Html.DisplayFor(modelItem => item.Description)
                </div>
                <div class="col-lg-4">
                    <img src="~/uploads/@item.ProfilePicture" class="profilePicture" />
                </div>
            </div>
        </div>
    </div>

}
    
